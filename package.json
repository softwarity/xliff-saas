{
  "name": "xliff-saas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "generate-languages": "node utils/generate-languages.js",
    "prestart": "node utils/generate-languages.js",
    "prebuild": "node utils/generate-languages.js",
    "preserver": "node utils/generate-languages.js",
    "start": "ng serve --no-hmr --serve-path /en/",
    "server": "lite-server -ws ./dist/xliff-translator/browser/",
    "dev": "ng serve --no-hmr --help",
    "extract-i18n": "ng extract-i18n",
    "start:fr": "ng serve --configuration=fr",
    "build:fr": "ng build --configuration=fr",
    "build": "ng build --localize",
    "serve:ssr:xliff-translator": "node dist/xliff-translator/server/server.mjs"
  },
  "dependencies": {
    "@angular/animations": "^19.2.7",
    "@angular/cdk": "^19.2.10",
    "@angular/common": "^19.2.7",
    "@angular/compiler": "^19.2.7",
    "@angular/core": "^19.2.7",
    "@angular/forms": "^19.2.7",
    "@angular/localize": "^19.2.7",
    "@angular/platform-browser": "^19.2.7",
    "@angular/platform-browser-dynamic": "^19.2.7",
    "@angular/platform-server": "^19.2.7",
    "@angular/router": "^19.2.7",
    "@angular/ssr": "^19.2.8",
    "@stripe/stripe-js": "^7.1.0",
    "@supabase/supabase-js": "^2.49.4",
    "express": "^4.18.2",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.8",
    "@angular/cli": "^19.2.8",
    "@angular/compiler-cli": "^19.2.7",
    "@types/express": "^4.17.17",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.21",
    "lite-server": "^2.6.1",
    "ng-extract-i18n-merge": "^2.14.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.3"
  }
}
